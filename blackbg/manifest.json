{
  "manifest_version": 2,
  "name": "blackbg",
  "version": "1.0",
  "description": "Build an Extension!",
  "options_page": "options.html",
  "browser_action": {
          "default_title": "Turn off the light",     
          "default_popup": "popup.html"       
        },
  "permissions": [
          "storage",
		  "tabs"
        ],
  "content_scripts": [
   {
     "matches": [
	 "<all_urls>"
	 ],
	 "js": [
	 "jquery-3.4.1.min.js",
	 "jquery-ui.min.js",
	 "blackbg.js"
	 ]
   }
  ]
}
